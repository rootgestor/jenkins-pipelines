pipeline {
  agent any
  stages {
    stage('Install frontend dependencies') {
      steps {
        sh 'cd frontend'
        yarn 'yarn'
      }
    }

    stage('build frontend') {
      steps {
        echo 'yarn build'
      }
    }

    stage('Publish') {
      steps {
        echo 'copy files to AWS'
      }
    }

    stage('Clean') {
      steps {
        echo 'Clear files'
      }
    }

  }
}